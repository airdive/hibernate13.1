
  CREATE TABLE "SCOTT"."STUDENT" 
   (	"ID" NUMBER(18,0) NOT NULL ENABLE, 
	"STUNAME" VARCHAR2(200 BYTE) NOT NULL ENABLE, 
	"SEX" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"AGE" NUMBER(3,0), 
	 CONSTRAINT "TABLE1_PK" PRIMARY KEY ("ID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 NOCOMPRESS LOGGING
  TABLESPACE "USERS"  ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  TABLESPACE "USERS" ;
 

  CREATE TABLE "SCOTT"."COURSE" 
   (	"ID" NUMBER(18,0) NOT NULL ENABLE, 
	"COURSENAME" VARCHAR2(200 BYTE) NOT NULL ENABLE, 
	"DESCRIPTION" VARCHAR2(200 BYTE), 
	"STUID" NUMBER(18,0) NOT NULL ENABLE, 
	 CONSTRAINT "COURSE_PK" PRIMARY KEY ("ID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 NOCOMPRESS LOGGING
  TABLESPACE "USERS"  ENABLE, 
	 CONSTRAINT "COURSE_FK1" FOREIGN KEY ("STUID")
	  REFERENCES "SCOTT"."STUDENT" ("ID") ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  TABLESPACE "USERS" ;
 
 